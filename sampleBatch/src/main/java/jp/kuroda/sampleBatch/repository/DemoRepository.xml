<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org/DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="jp.kuroda.sampleBatch.repository.DemoRepository">
	<select id="select" resultType="jp.kuroda.sampleBatch.model.DemoDTO" fetchSize="${fetch-size}">
		SELECT
			id,
			name,
			mail_address as mailAddress
		FROM
			users
	</select>
</mapper>